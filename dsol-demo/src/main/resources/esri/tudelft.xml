<?xml version="1.0" encoding="UTF-8" ?>
<!-- Created by Alexander Verbraeck on Apr 30, 2014 -->
<map xmlns:gismap="http://www.simulation.tudelft.nl/esri">

	<!-- We start with the global map attributes -->
	<name>tudelft</name>
	<units>dd</units>
	<extent>
		<minX>-122.071</minX>
		<minY>37.402</minY>
		<maxX>-121.992</maxX>
		<maxY>37.435</maxY>
	</extent>
	<image>
		<backgroundColor>
			<r>255</r>
			<g>255</g>
			<b>255</b>
		</backgroundColor>
		<size>
			<height>2000</height>
			<width>2000</width>
		</size>
	</image>

	<!-- Now we get the layers -->
  <layer>
    <name>natural</name>
    <data>
      <shapeFile>/resources/esri/tudelft/natural.shp</shapeFile>
    </data>
    <fillColor>
      <r>127</r>
      <g>255</g>
      <b>197</b>
    </fillColor>
    <outlineColor>
      <r>0</r>
      <g>255</g>
      <b>0</b>
    </outlineColor>
    <display>true</display>
    <transform>true</transform>
  </layer>
	<layer>
		<name>roads</name>
		<data>
			<shapeFile>/resources/esri/tudelft/roads.shp</shapeFile>
		</data>
		<outlineColor>
			<r>120</r>
			<g>120</g>
			<b>120</b>
		</outlineColor>
		<display>true</display>
		<transform>true</transform>
	</layer>
	<layer>
		<name>buildings</name>
		<data>
			<shapeFile>/resources/esri/tudelft/buildings.shp</shapeFile>
		</data>
		<fillColor>
			<r>200</r>
			<g>200</g>
			<b>200</b>
		</fillColor>
		<outlineColor>
			<r>200</r>
			<g>200</g>
			<b>200</b>
		</outlineColor>
		<display>true</display>
		<transform>true</transform>
	</layer>
	<layer>
		<name>railways</name>
		<data>
			<shapeFile>/resources/esri/tudelft/railways.shp</shapeFile>
		</data>
		<outlineColor>
			<r>0</r>
			<g>19</g>
			<b>127</b>
		</outlineColor>
		<display>true</display>
		<transform>true</transform>
	</layer>
	<layer>
		<name>waterways</name>
		<data>
			<shapeFile>/resources/esri/tudelft/waterways.shp</shapeFile>
		</data>
		<outlineColor>
			<r>0</r>
			<g>148</g>
			<b>255</b>
		</outlineColor>
		<display>true</display>
		<transform>true</transform>
	</layer>
</map>
