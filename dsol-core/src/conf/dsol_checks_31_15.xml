      <property name="severity" value="ignore"/>
      <property name="tokens" value="PARAMETER_DEF, VARIABLE_DEF"/>
    </module>
    <module name="IllegalInstantiation"/>
    <module name="InnerAssignment"/>
    <module name="MagicNumber">
      <property name="severity" value="ignore"/>
